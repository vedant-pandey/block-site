<svg width="128" height="128" viewBox="0 0 128 128" xmlns="http://www.w3.org/2000/svg">
  <!-- Complex gradients and effects -->
  <defs>
    <radialGradient id="mainGradient" cx="0.3" cy="0.3" r="0.9">
      <stop offset="0%" stop-color="#ff7675"/>
      <stop offset="50%" stop-color="#ff6b7a"/>
      <stop offset="100%" stop-color="#ff4757"/>
    </radialGradient>
    <linearGradient id="blockGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#fff"/>
      <stop offset="100%" stop-color="#f1f2f6"/>
    </linearGradient>
    <filter id="outerShadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="2" dy="2" stdDeviation="3" flood-color="#000" flood-opacity="0.4"/>
    </filter>
    <filter id="innerShadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="1" dy="1" stdDeviation="1" flood-color="#000" flood-opacity="0.2"/>
    </filter>
  </defs>
  
  <!-- Main background circle -->
  <circle cx="64" cy="64" r="56" fill="url(#mainGradient)" stroke="#fff" stroke-width="3" filter="url(#outerShadow)"/>
  
  <!-- Inner accent circle -->
  <circle cx="64" cy="64" r="50" fill="none" stroke="rgba(255,255,255,0.3)" stroke-width="1"/>
  
  <!-- Main block symbol -->
  <circle cx="64" cy="64" r="40" fill="none" stroke="url(#blockGradient)" stroke-width="6" filter="url(#innerShadow)"/>
  <line x1="32" y1="32" x2="96" y2="96" stroke="url(#blockGradient)" stroke-width="8" stroke-linecap="round" filter="url(#innerShadow)"/>
  
  <!-- Inner detail circles -->
  <circle cx="64" cy="64" r="30" fill="none" stroke="rgba(255,255,255,0.6)" stroke-width="2"/>
  <circle cx="64" cy="64" r="20" fill="none" stroke="rgba(255,255,255,0.4)" stroke-width="1"/>
  
  <!-- Timer markers (clock-like) -->
  <g stroke="#fff" stroke-width="2" opacity="0.8">
    <!-- 12 o'clock -->
    <line x1="64" y1="16" x2="64" y2="24"/>
    <!-- 3 o'clock -->
    <line x1="112" y1="64" x2="104" y2="64"/>
    <!-- 6 o'clock -->
    <line x1="64" y1="112" x2="64" y2="104"/>
    <!-- 9 o'clock -->
    <line x1="16" y1="64" x2="24" y2="64"/>
  </g>
  
  <!-- Corner accent dots -->
  <circle cx="64" cy="20" r="3" fill="rgba(255,255,255,0.9)"/>
  <circle cx="108" cy="64" r="3" fill="rgba(255,255,255,0.9)"/>
  <circle cx="64" cy="108" r="3" fill="rgba(255,255,255,0.9)"/>
  <circle cx="20" cy="64" r="3" fill="rgba(255,255,255,0.9)"/>
  
  <!-- Central focus point -->
  <circle cx="64" cy="64" r="4" fill="rgba(255,255,255,0.9)" stroke="none"/>
</svg>
